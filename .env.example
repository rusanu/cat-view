# AWS S3 Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git - it contains secrets!

AWS_REGION=us-east-1
AWS_BUCKET_NAME=your-bucket-name
AWS_BUCKET_FOLDER=your-folder-name

# Google OAuth OIDC Configuration (REQUIRED)
# Used for user authentication
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# AWS Cognito Identity Pool Configuration (REQUIRED)
# Used to exchange Google OIDC tokens for temporary AWS credentials
# Static AWS credentials (AWS_ACCESS_KEY_ID/AWS_SECRET_ACCESS_KEY) are no longer supported
COGNITO_IDENTITY_POOL_ID=us-east-1:your-identity-pool-id
