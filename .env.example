# AWS S3 Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git - it contains secrets!

AWS_REGION=us-east-1
AWS_BUCKET_NAME=your-bucket-name
AWS_BUCKET_FOLDER=your-folder-name
# NOTE: AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY are deprecated
# They will be replaced by Cognito Identity Pool authentication
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Google OAuth OIDC Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# AWS Cognito Identity Pool Configuration
# This will be used to exchange Google OIDC tokens for AWS credentials
COGNITO_IDENTITY_POOL_ID=us-east-1:your-identity-pool-id
